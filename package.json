{
  "name": "economicdata.co",
  "version": "0.1.0",
  "description": "EconomicData.co site.",
  "repository": "https://github.com/joshweir/economicdata.co",
  "main": "index.js",
  "scripts": {
    "sequelize": "node_modules/.bin/sequelize",
    "lint": "eslint --ext .js,.jsx app server",
    "lint:fix": "eslint --ext .js,.jsx app server --fix",
    "clean": "rimraf public && rimraf compiled",
    "start": "npm run build && cross-env NODE_ENV=production node compiled/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build:dev": "cross-env NODE_ENV=development npm run webpack -- --env.server",
    "build": "npm run clean && cross-env NODE_ENV=production npm run webpack -- --env.browser && cross-env NODE_ENV=production npm run webpack -- --env.server",
    "webpack": "webpack --colors --display-error-details --config ./webpack/webpack.config.js",
    "test": "mocha --require babel-register --require ./app/tests/setup.js --compilers css:./app/tests/compilers/css ./app/**/*-test.js ./server/**/*-test.js",
    "test:coverage": "cross-env NODE_ENV=nyc nyc -all -e .jsx mocha --require ./app/tests/setup.js --compilers css:./app/tests/compilers/css ./app/**/*-test.js ./server/**/*-test.js",
    "test:travis": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls",
    "test:watch": "mocha --require babel-register --require ./app/tests/setup.js --watch --compilers css:./app/tests/compilers/css ./app/**/*-test.js ./server/**/*-test.js"
  },
  "author": "joshweir",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-register": "^6.24.0",
    "coveralls": "^3.0.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
