    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.15.2",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.10.2",
    "react-test-renderer": "^15.5.3",
    "react-transform-catch-errors": "^1.0.2",
    "redux-mock-store": "1.2.2",
    "sinon": "^2.1.0"
  },
  "engines": {
    "node": ">=6.x"
  },
