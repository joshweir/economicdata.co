    "slugify": "^1.2.9",
    "spark-md5": "^3.0.0",
    "spawngo": "^2.0.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "nyc": {
    "include": [
      "app/**",
      "server/db/mongo/models/**"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "app/tests/**",
      "app/store/**"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcov",
      "html"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "nyc": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  }
}
